<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Android-App OLXPH" verbose="1" parallel="tests" thread-count="1">
	
	<plugins>
		<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-surefire-plugin</artifactId>
			<version>2.18.1</version>
			<configuration>
				<suiteXmlFiles>
				  	<suiteXmlFile>testng.xml</suiteXmlFile>
				</suiteXmlFiles>
			</configuration>
		</plugin>
	</plugins> 
			
	<test name="Account Logged In Test" preserve-order="false">
	    <!-- <parameter name="udid" value="192.168.57.101:5555"/> -->
		<classes>
			<class name="com.olx.TestLoginContext"/>
		</classes>
	</test>
	
</suite>

